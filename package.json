{
  "name": "express-es6",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prestart": "yarn run build",
    "start": "node -r dotenv/config dist",
    "build": "babel src -s -D -d dist",
    "dev": "nodemon -w src --exec \"node -r dotenv/config -r babel-register src\""
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-prettier": "^2.1.2",
    "nodemon": "^1.11.0",
    "prettier": "^1.5.2"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "morgan": "^1.8.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": { "node": true }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
      "ecmaVersion": 7
    },
    "rules": {
      "prettier/prettier": "error"
    }
  }
}
