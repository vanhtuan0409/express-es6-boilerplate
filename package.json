{
  "name": "express-es6",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prestart": "yarn run build",
    "start": "node dist",
    "build": "babel src -s -D -d dist",
    "dev": "nodemon -w src --exec \"babel-node src\""
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "dotenv": "^4.0.0",
    "eslint": "^4.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "nodemon": "^1.11.0",
    "prettier": "^1.5.2"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "morgan": "^1.8.2"
  },
  "babel": {
    "presets": ["env"]
  },
  "eslintConfig": {
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error"
    }
  }
}
